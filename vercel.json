{
  "version": 2,
  "builds": [
    {
      "src": "api/scores.py",
      "use": "@vercel/python"
    },
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/scores",
      "dest": "/api/scores.py"
    },
    {
      "src": "/(.*)",
      "dest": "/build/web/$1"
    }
  ]
}
